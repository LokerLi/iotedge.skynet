[Unit]
Description=Frp Client Service
Wants=network.target
After=network.target

[Service]
WorkingDirectory=WORKING_DIR
ExecStart=WORKING_DIR/bin/prebuilt/frpc -c WORKING_DIR/run/frpc.ini
ExecReload=WORKING_DIR/bin/prebuilt/frpc reload -c WORKING_DIR/run/frpc.ini
RestartSec=5s
Restart=on-failure

[Install]
WantedBy=multi-user.target
